	IDCorral,
	SUM(Cantidad)
FROM
    tblMovimientoAnimales
GROUP BY
	IDCorral,Fecha
HAVING
 (Fecha BETWEEN '2021-01-13' AND '2021-02-01' )
 
 SELECT tblMovimientoAnimales.IDMovimiento, tblCorrales.ID , tblCorrales.Descripcion, SUM(Cantidad) 
 FROM tblMovimientoAnimales
 INNER JOIN tblCorrales ON  tblMovimientoAnimales.IDCorral=tblCorrales.ID
 WHERE (Fecha BETWEEN '2021-01-01' AND '2021-02-14' )
 GROUP BY IDCorral, IDMovimiento
 HAVING  IDCliente=1 

 
 
 SELECT  tblProductos.Descripcion,  tblCorrales.Descripcion,  tblProductos.IDUnidadMedida, SUM(tblDetalleOrdenSurtido.CantSurtida)
 FROM tblDetalleOrdenSurtido
 INNER JOIN tblProductos ON tblOrdenesSurtido.IDProducto=tblProductos.ID
 INNER JOIN tblOrdenesSurtido ON tblDetalleOrdenSurtido.IDOrden=tblOrdenesSurtido.ID
 INNER JOIN tblCorrales ON tblDetalleOrdenSurtido.IDCorral=tblCorrales.ID
 WHERE (Fecha BETWEEN '2021-01-13' AND '2021-05-05' )
 GROUP BY   IDCorral,tblProductos.ID
 HAVING IDCliente=1 
 
SELECT  tblProductos.Descripcion,  tblCorrales.Descripcion,  tblProductos.IDUnidadMedida, SUM(tblDetalleOrdenSurtido.CantSurtida)
 FROM tblDetalleOrdenSurtido
 INNER JOIN tblProductos ON tblOrdenesSurtido.IDProducto=tblProductos.ID
 INNER JOIN tblOrdenesSurtido ON tblDetalleOrdenSurtido.IDOrden=tblOrdenesSurtido.ID
 INNER JOIN tblCorrales ON tblDetalleOrdenSurtido.IDCorral=tblCorrales.ID
 WHERE (Fecha BETWEEN '2021-01-13' AND '2021-02-05' )
 GROUP BY   tblProductos.ID
 HAVING IDCliente=1 

SELECT  Folio, Fecha, tblDetalleMovimientosMP.IDMP, tblMovimientosMP.TipoMov, 
        tblMovimientosMP.IDProveedor
FROM tblMovimientosMP
INNER JOIN tblMateriaPrima ON tblDetalleMovimientosMP.IDMP=tblMateriaPrima.ID
INNER JOIN tblMovimientosMP ON tblDetalleMovimientosMP.IDFolio=tblMovimientosMP.Folio
WHERE  tblMovimientosMP.IDAlmacen= 1 AND (Fecha BETWEEN '2021-01-01' AND '2021-05-05' )
********************************************************
SELECT  tblMovimientosMP.Folio, tblMovimientosMP.Fecha,tblMovimientosMP.TipoMov, tblProveedores.Nombre AS Proveedor,
        tblDetalleMovimientosMP.IDMP, tblMateriaPrima.Descripcion AS MP,  SUM(tblDetalleMovimientosMP.Cantidad)
FROM  tblMovimientosMP
INNER JOIN tblDetalleMovimientosMP ON tblDetalleMovimientosMP.IDFolio=tblMovimientosMP.Folio
INNER JOIN tblMateriaPrima ON tblMateriaPrima.ID=tblDetalleMovimientosMP.IDMP
INNER JOIN tblProveedores ON tblProveedores.ID = tblMovimientosMP.IDProveedor
WHERE   IDAlmacen= 3 AND (tblMovimientosMP.Fecha BETWEEN '2021-01-01' AND '2021-05-24' )
GROUP BY  TipoMov
***********************************************************
 
